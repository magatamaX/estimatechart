{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "no-tabs": 0,
    "default-case": 0,
    "no-restricted-properties": 0,
    "jsx-a11y/media-has-caption": 0,
    "jsx-a11y/alt-text": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "react/style-prop-object": 0,
    "react/forbid-prop-types": 0,
    "no-unused-vars": ["warn"]
  },
  "globals": {
    "document": true
  }
}